<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <FrameLayout
            android:id="@+id/layout_Tmap"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginStart="10dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="10dp"
                android:background="#00FFFFFF"
                android:elevation="3dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="3dp"
                    android:layout_weight="1"
                    android:background="@drawable/rect_round_edit"
                    android:elevation="3dp"
                    android:orientation="horizontal"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp">

                    <ImageButton
                        android:id="@+id/btn_menu"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center"
                        android:layout_marginStart="7dp"
                        android:backgroundTint="#00FFFFFF"
                        android:src="@drawable/ic_baseline_menu_24"
                        app:tint="#505050" />

                    <TextView
                        android:id="@+id/tv_search_address"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:background="#00FFFFFF"
                        android:ems="18"
                        android:gravity="center_vertical"
                        android:maxLines="1"
                        android:text="주소 검색"
                        android:textColor="#ACACAC"
                        android:textSize="13.5sp" />


                </LinearLayout>

                <ImageButton
                    android:id="@+id/btn_streetfind"
                    android:layout_width="50dp"
                    android:layout_height="52dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/rect_round_button"
                    android:backgroundTint="#0060F1"
                    android:elevation="3dp"
                    android:padding="10dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/ic_btn_streetfind" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|top"
                android:layout_marginTop="100dp"
                android:layout_marginEnd="5dp"
                android:background="#00FFFFFF"
                android:elevation="3dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btn_light"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:padding="7dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/layout_round"
                    android:elevation="3dp"
                    android:gravity="center"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_light"
                    android:text="전체"
                    android:textColor="#5E5E5E" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|start|bottom"
                android:layout_marginStart="15dp"
                android:layout_marginBottom="120dp"
                android:background="#00FFFFFF"
                android:elevation="3dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btn_all"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop"
                    android:src="@drawable/ic_round_keyboard_double_arrow_up_24"
                    android:tint="#000000"
                    tools:ignore="UseAppTint" />
                <!--
                                <TextView
                                    android:id="@+id/btn_human_light1"
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_margin="7dp"
                                    android:background="@drawable/layout_round_green"
                                    android:elevation="3dp"
                                    android:gravity="center"
                                    android:textColor="#5E5E5E" />

                                <TextView
                                    android:id="@+id/btn_human_light2"
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_margin="7dp"
                                    android:background="@drawable/layout_round_yellow"
                                    android:elevation="3dp"
                                    android:gravity="center"
                                    android:textColor="#5E5E5E" />

                                <TextView
                                    android:id="@+id/btn_human_light3"
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_margin="7dp"
                                    android:background="@drawable/layout_round_red"
                                    android:elevation="3dp"
                                    android:gravity="center"
                                    android:textColor="#5E5E5E" />-->

                <TextView
                    android:id="@+id/btn_human_light"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_margin="7dp"
                    android:background="@drawable/layout_round"
                    android:elevation="3dp"
                    android:gravity="center"
                    android:text="전체"
                    android:textColor="#5E5E5E" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|center|bottom"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="120dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/btn_max"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/ic_round_add_24"
                    app:tint="#A5A5A5" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#DDDDDD" />

                <ImageButton
                    android:id="@+id/btn_min"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:backgroundTint="@color/white"
                    android:src="@drawable/ic_round_remove_24"
                    app:tint="#A5A5A5" />

            </LinearLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="18dp"
                android:background="@drawable/rect_round_edit"
                android:backgroundTint="@color/white"
                android:elevation="2dp"
                android:orientation="horizontal"
                android:paddingTop="8dp"
                android:paddingBottom="8dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginStart="40dp"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btn_shareLocation"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:background="#00FFFFFF"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_outline_share_24"
                        app:tint="#2B2B2B" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="3dp"
                        android:text="내 위치 공유"
                        android:textColor="@color/black"
                        android:textSize="10sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginEnd="35dp"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btn_bookmark"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:background="#00FFFFFF"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_outline_bookmark_added_24"
                        app:tint="#2B2B2B" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="3dp"
                        android:text="즐겨찾기"
                        android:textColor="@color/black"
                        android:textSize="10sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="50dp"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btn_myLocation"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:background="#00FFFFFF"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_outline_my_location_24"
                        app:tint="#2B2B2B" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="3dp"
                        android:text="현 위치"
                        android:textColor="@color/black"
                        android:textSize="10sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginEnd="40dp"
                    android:orientation="vertical">

                    <ImageButton
                        android:id="@+id/btn_call"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_gravity="center"
                        android:background="#00FFFFFF"
                        android:scaleType="fitCenter"
                        android:src="@drawable/ic_baseline_call_24"
                        app:tint="#E30202" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="3dp"
                        android:text="긴급 전화"
                        android:textColor="#E30202"
                        android:textSize="10sp" />

                </LinearLayout>

            </FrameLayout>

        </FrameLayout>

    </LinearLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigation_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/drawer_header"
        app:menu="@menu/drawer_menu" />

</androidx.drawerlayout.widget.DrawerLayout>